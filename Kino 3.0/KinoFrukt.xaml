            </FlowDocument>
        </RichTextBox>
        <Button x:Name="btnYtbSet" Content="Youtube Set" HorizontalAlignment="Left" Margin="734,9,0,0" VerticalAlignment="Top" Width="78" Click="btnYtbSet_Click"/>
        <TextBox x:Name="tbYtbNumber" HorizontalAlignment="Left" Height="19" Margin="817,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="19" RenderTransformOrigin="1.398,0.314"/>
        <Button x:Name="btnYTBEng" Content="Eng" HorizontalAlignment="Left" Margin="845,10,0,0" VerticalAlignment="Top" Width="34" Click="btnYTBEng_Click"/>
        <Button x:Name="btnYtbGet" Content="Youtube Get" HorizontalAlignment="Left" Margin="643,10,0,0" VerticalAlignment="Top" Width="73" Click="btnYtbGet_Click"/>
        <Button x:Name="btnResult" Content="Result" HorizontalAlignment="Left" Margin="416,443,0,0" VerticalAlignment="Top" Width="58" Click="btnResult_Click" BorderBrush="{x:Null}" Background="#FF12A624"/>

    </Grid>
